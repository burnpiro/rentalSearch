{
  "manifest_version": 2,
  "name": "Gumtree/Olx rentals live feed",
  "description": "Live updates about gumtree/olx offers",
  "version": "0.0.1",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Rental watch",
    "default_popup": "index.html"
  },
  "permissions": [
    "http://www.gumtree.pl/",
    "http://olx.pl/",
    "storage"
  ],
  "content_security_policy": "script-src 'unsafe-eval'; object-src 'unsafe-eval'"
}